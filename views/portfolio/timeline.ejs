<% layout('layouts/boilerplate')%>

<div class="container">
  <div class="item">
    <div id="timeline">
      <div>
        <%for(let i=0; i<eduArr.length; i++){%>
          <section class="year">
            <h3><%= eduArr[i].year %></h3>
            <%for(let j=0; j<eduArr[i].details.length; j++){%>
              <section>
                <ul>
                  <li><%= eduArr[i].details[j].title %>
                    <p><%= eduArr[i].details[j].courseProvider%> <br> <a title="Certificate" class="btn-edu" href= <%= eduArr[i].details[j].certi %> ><i class="fas fa-award"></i></a>
                    </p>
                  </li>
                </ul>
              </section>
            <%}%>
          </section>
        <%}%>
      </div>
    </div>
  </div>
</div>

<script src="/jscript/timeline.js"></script>